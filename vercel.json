{
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist/spa",
  "framework": null,
  "installCommand": "pnpm install",
  "devCommand": "pnpm dev",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}

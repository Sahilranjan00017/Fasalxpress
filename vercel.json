{
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist/spa",
  "framework": null,
  "installCommand": "pnpm install",
  "devCommand": "pnpm dev",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
